{
  "name": "stinky-wiki",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "pnpm exec prettier . --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*": [
      "pretty-quick --staged",
      "git add"
    ]
  },
  "dependencies": {
    "astro": "^5.13.2"
  },
  "devDependencies": {
    "eslint": "^9.34.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "pretty-quick": "^4.2.2",
    "slint": "^0.1.0"
  }
}
